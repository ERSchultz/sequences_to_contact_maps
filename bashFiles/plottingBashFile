#! /bin/bash
#SBATCH --job-name=plotting
#SBATCH --partition=depablo-gpu
#SBATCH --nodes=1
#SBATCH --ntasks-per-node=1
#SBATCH --mem-per-cpu=2000
#SBATCH --gres=gpu:1
#SBATCH --output=logFiles/plotting.log
#SBATCH --time=03:00:00

dirname="../../../project2/depablo/erschultz/dataset_04_18_21"

# partition
gpus=1

# model
modelType='GNNAutoencoder'
pretrained='true'

# other
plotPredictions='true'
useScratch='true'


cd ~/sequences_to_contact_maps
source activate python3.8_pytorch1.8.1_cuda10.2


python3 plotting_functions.py  --data_folder $dirname --model_type $modelType --gpus $gpus --id $id --pretrained $pretrained --plot_predictions $plotPredictions --use_scratch $useScratch



python3 cleanDirectories.py --data_folder $dirname --root_name $modelType --use_scratch $useScratch
