#! /bin/bash
#SBATCH --job-name=plotting
#SBATCH --partition=depablo-gpu
#SBATCH --nodes=1
#SBATCH --ntasks-per-node=4
#SBATCH --mem-per-cpu=2000
#SBATCH --gres=gpu:1
#SBATCH --output=logFiles/plotting.log
#SBATCH --time=03:00:00

dirname="../../../project2/depablo/erschultz/dataset_04_18_21"

# partition
gpus=1

# model
modelType='UNet'
pretrained='true'

# other
plotPredictions=True


cd ~/sequences_to_contact_maps
source activate seq2contact_pytorch

modelType='UNet'
for id in 1 2 3 4 5 6 13 14 15 22 23 24 25 26 27
do
  python3 plotting_functions.py  --data_folder $dirname  --gpus $gpus --model_type $modelType --id $id --pretrained $pretrained
done

modelType='DeepC'
for id in 1 2 3 7 8 9
do
  python3 plotting_functions.py  --data_folder $dirname  --gpus $gpus --model_type $modelType --id $id --pretrained $pretrained
done
